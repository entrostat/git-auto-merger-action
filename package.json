{
  "name": "git-auto-merger-action",
  "version": "0.0.0",
  "description": "A Github Action that tries to merge a base branch into branches you specify and sends an alert if it is not able to do that.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "entro-version --master-branch-name=main",
    "release:major": "entro-version --master-branch-name=main --standard-version-flags=\"--release-as=major\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/entrostat/git-auto-merger-action.git"
  },
  "keywords": [
    "git",
    "auto-merge",
    "action",
    "github",
    "pipeline",
    "notification",
    "git-flow",
    "release",
    "publish",
    "ci",
    "cd"
  ],
  "author": "Kerren-Entrostat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/entrostat/git-auto-merger-action/issues"
  },
  "homepage": "https://github.com/entrostat/git-auto-merger-action#readme",
  "devDependencies": {
    "entro-version": "^1.7.0"
  }
}
